# assign clientID variable
create_clientID add_proxy "alsa_out"

create_proxy --label "alsa_out" --arguments "-- -j out_$device -d h:$device"

# set client properties
set_client_properties --launched "1" --description "Playback device $device" --icon "alsa_out_x11" --label "alsa_out" --name "alsa_out"

set_jackclient_properties --jackclientname "out_$device"

#end for
#end if

